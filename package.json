{
  "name": "nlac-student-grade-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://ilocanoproud06-ops.github.io/NLACStudentGradeTracker/",
  "scripts": {
    "dev": "vite",
    "dev:student": "vite --open student.html",
    "build": "vite build",
    "build:student": "vite build",
    "preview": "vite preview",
    "preview:student": "vite preview --open student.html",
    "predeploy": "vite build",
    "deploy": "gh-pages -d dist",
    "deploy:student": "vite build && gh-pages -d dist -e student",
    "deploy:firebase": "vite build && firebase deploy",
    "deploy:student:firebase": "vite build && firebase deploy --only hosting:student"
  },
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "firebase": "^12.9.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.5"
  }
}
